<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.finery">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PreviousRates"></activity>
        <activity android:name=".Feedback" />
        <activity android:name=".DeliveryDetails" />
        <activity android:name=".CartActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddNewProduct" />
        <activity
            android:name=".ProductAdmin"
            android:label="@string/title_activity_product_admin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ProductCustomerMoreDetails" />
        <activity android:name=".ProductCustomerView" />
        <activity android:name=".GiveRates" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>